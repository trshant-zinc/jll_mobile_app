<?xml version='1.0' encoding='utf-8'?>
<widget id="org.admin247.jll" version="0.0.3" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>JLL</name>
    <description>
        JLL Resource All
    </description>
    <author email="techinternal@zincdigital.com" href="http://www.zincdigital.com/">Ganesh Kamath</author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <icon src="www/images/favicon.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="~5.1.1" />
    <plugin name="cordova-plugin-network-information" spec="~1.2.1" />
    <plugin name="cordova-plugin-file" spec="~4.2.0" />
</widget>
